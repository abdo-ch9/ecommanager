<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Dashboard Background -->
  <rect width="600" height="400" fill="#1e293b" rx="12"/>
  
  <!-- Header Bar -->
  <rect x="0" y="0" width="600" height="60" fill="#0f172a" rx="12"/>
  <rect x="20" y="20" width="120" height="20" fill="#3b82f6" rx="4"/>
  <circle cx="550" cy="30" r="15" fill="#10b981"/>
  
  <!-- Sidebar -->
  <rect x="0" y="60" width="180" height="340" fill="#334155"/>
  <rect x="20" y="80" width="140" height="12" fill="#64748b" rx="2"/>
  <rect x="20" y="100" width="100" height="12" fill="#64748b" rx="2"/>
  <rect x="20" y="120" width="120" height="12" fill="#3b82f6" rx="2"/>
  <rect x="20" y="140" width="110" height="12" fill="#64748b" rx="2"/>
  
  <!-- Main Content Area -->
  <rect x="200" y="80" width="380" height="300" fill="#475569" rx="8"/>
  
  <!-- Charts and Widgets -->
  <rect x="220" y="100" width="160" height="120" fill="#1e293b" rx="6"/>
  <rect x="400" y="100" width="160" height="120" fill="#1e293b" rx="6"/>
  
  <!-- Chart Lines -->
  <polyline points="240,180 260,160 280,140 300,120 320,100 340,110 360,90" 
            fill="none" stroke="#3b82f6" stroke-width="3"/>
  
  <!-- Bar Chart -->
  <rect x="420" y="180" width="15" height="30" fill="#10b981"/>
  <rect x="440" y="160" width="15" height="50" fill="#10b981"/>
  <rect x="460" y="140" width="15" height="70" fill="#3b82f6"/>
  <rect x="480" y="170" width="15" height="40" fill="#10b981"/>
  <rect x="500" y="150" width="15" height="60" fill="#3b82f6"/>
  <rect x="520" y="130" width="15" height="80" fill="#3b82f6"/>
  
  <!-- Bottom Widgets -->
  <rect x="220" y="240" width="100" height="60" fill="#1e293b" rx="6"/>
  <rect x="340" y="240" width="100" height="60" fill="#1e293b" rx="6"/>
  <rect x="460" y="240" width="100" height="60" fill="#1e293b" rx="6"/>
  
  <!-- Status Indicators -->
  <circle cx="250" cy="270" r="8" fill="#10b981"/>
  <circle cx="370" cy="270" r="8" fill="#f59e0b"/>
  <circle cx="490" cy="270" r="8" fill="#3b82f6"/>
  
  <!-- Text Labels -->
  <text x="240" y="285" fill="#94a3b8" font-family="Arial" font-size="10">Active</text>
  <text x="360" y="285" fill="#94a3b8" font-family="Arial" font-size="10">Pending</text>
  <text x="480" y="285" fill="#94a3b8" font-family="Arial" font-size="10">Complete</text>
</svg>